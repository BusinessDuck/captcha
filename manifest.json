{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "version": "0.1",
  "name": "reCaptcha Breaker",
  "externally_connectable": {
    "ids": ["*"],
    "matches": [
      "http://*.raiblockscommunity.net/*",
      "*://google.com/*"
    ],
    "accepts_tls_channel_id": true
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery.js",
        "content.js"
      ],
      "all_frames": true
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "web_accessible_resources": [
    "/inject.js"
  ],
  "permissions": [
    "tabs",
    "activeTab",
    "webRequest",
    "webRequestBlocking",
    "https://faucet.raiblockscommunity.net/*",
    "*://google.com/*"
  ]
}